{
  "extends": [
    "next/core-web-vitals",
    "google",
    "prettier",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "object-curly-spacing": [
      2,
      "always"
    ],
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": true
      }
    ],
    "react/no-unknown-property": [
      2,
      {
        "ignore": [
          "jsx"
        ]
      }
    ],
    "no-empty": "off",
    "no-undef": "off",
    "require-jsdoc": "off",
    "new-cap": "off",
    "react/prop-types": "off"
  }
}